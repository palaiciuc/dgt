{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,jpg,jpeg,svg,ico,webp,eot,otf,ttf,woff,woff2}"
    ]
  },
  "mimeTypes": {
    ".webp": "image/webp",
    ".weba": "audio/webp"
  }
}
